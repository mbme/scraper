{
  "name": "scraper",
  "version": "2.6.0",
  "description": "Scrape the data from the page you're on",
  "main": "index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit --project tsconfig.json",
    "build": "tsx build.ts",
    "prod:build": "NODE_ENV=production npm run build",
    "build:typings": "npx tsc --declaration --emitDeclarationOnly --outDir ./types",
    "lint": "eslint --cache --ext .ts src/",
    "fmt": "prettier --write src/",
    "check-fmt": "prettier --check src/",
    "test": "",
    "preversion": "npm run check-types && npm run lint && npm run check-fmt",
    "version": "npm run build && cp dist/scraper.user.js* docs/ && git add -f docs/scraper.user.js* && npm run build:typings && git add -f types/",
    "postversion": "git push && git push --tags"
  },
  "author": "mbme",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.12",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.0.0",
    "puppeteer-core": "^22.14.0",
    "tsx": "^4.5.0",
    "typescript": "~5.5.0",
    "typescript-eslint": "^7.17.0",
    "urlpattern-polyfill": "^10.0.0"
  },
  "dependencies": {},
  "files": [
    "src/",
    "docs/",
    "types/"
  ]
}
